// IBM Design Language        //
// Color Palette              //
//                            //
////////////////////////////////

{{> color-functions.scss}}

//////////////////////////////
// IBM Color Palettes
//////////////////////////////
$__ibm-color-palettes: (
{{#each colors.palettes}}
  '{{name}}': (
{{#each values}}
    {{tone}}: {{value}}{{#unless @last}},{{/unless}}
{{/each}}
  ){{#unless @last}},{{/unless}}
{{/each}}
);
